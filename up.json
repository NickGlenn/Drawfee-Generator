{
  "name": "drawfee-roulette",
  "profile": "personal",
  "type": "static",
  "hooks": {
    "build": "npm run build:scripts && npm run build:styles"
  },
  "static": {
    "dir": "public"
  },
  "regions": [
    "us-west-2"
  ],
  "lambda": {
    "warm": true,
    "warm_count": 1,
    "warm_rate": "30m"
  },
  "stages": {
    "production": {
      "domain": "drawfee-generator.com"
    },
    "staging": {
      "domain": "staging.drawfee-generator.com"
    }
  }
}